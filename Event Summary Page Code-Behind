using EventosApp.Models;

namespace EventosApp.Views
{
    public partial class EventSummaryPage : ContentPage
    {
        // Construtor que recebe a Model Evento
        public EventSummaryPage(Evento evento)
        {
            InitializeComponent();
            
            // Define o objeto Evento como o BindingContext desta p√°gina,
            // permitindo que o XAML exiba suas propriedades.
            BindingContext = evento;
        }
    }
}
